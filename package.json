{
    "name": "desktopbrowser",
    "version": "1.0.0",
    "main": "server/out/server/index.js",
    "repository": "git@github.com:danelkhen/desktopbrowser.git",
    "author": "Dan-el Khen <danelkhen@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "npx concurrently \"npx --cwd server tsc -w --preserveWatchOutput\" \"npx --cwd client dev\" \"npx delay 4 && npx server\"",
        "server": "npx nodemon server/out/server.js -w server --delay 2",
        "build": "npx --cwd server && npx --cwd server tsc && npx --cwd client && npx --cwd client tsc",
        "start": "ts-node server/src/server.ts"
    },
    "dependencies": {
        "@babel/plugin-proposal-export-default-from": "^7.8.3",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "concurrently": "^6.0.0",
        "delay-cli": "^1.1.0",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "linaria": "^2.1.0",
        "nodemon": "^2.0.2",
        "prettier": "^2.2.1",
        "typescript": "^4.2.3",
        "webpack-cli": "^4.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.9.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.17.0"
    }
}
